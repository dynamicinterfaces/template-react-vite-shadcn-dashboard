{
  "framework": "vite",
  "packageManager": "npm",
  "devCommand": "npm run dev",
  "devPort": 5173,
  "buildCommand": "npm run build",
  "buildOutputDir": "dist",
  "routes": [
    { "filePath": "src/features/posts/index.tsx", "route": "/", "name": "Posts" },
    { "filePath": "src/features/posts/post-detail.tsx", "route": "/post/1", "name": "Post Detail" },
    { "filePath": "src/features/payment/index.tsx", "route": "/payment", "name": "Payment" },
    { "filePath": "src/features/kanban/index.tsx", "route": "/kanban", "name": "Kanban" },
    { "filePath": "src/features/kanban/create.tsx", "route": "/kanban/create", "name": "Kanban Create" },
    { "filePath": "src/features/kanban/detail.tsx", "route": "/kanban/detail/1", "name": "Kanban Detail" },
    { "filePath": "src/features/kanban/edit.tsx", "route": "/kanban/edit/1", "name": "Kanban Edit" },
    { "filePath": "src/features/chats/index.tsx", "route": "/chats", "name": "Chats" },
    { "filePath": "src/features/users/index.tsx", "route": "/users", "name": "Users" },
    { "filePath": "src/features/settings/index.tsx", "route": "/settings", "name": "Settings" }
  ]
}
